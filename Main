-- Scripts:

local function KKFXSHJ_fake_script() -- Join.Join 
	local script = Instance.new('LocalScript', Join)

	script.Parent.MouseButton1Down:Connect(function()
		local PlayerNamex = script.Parent.Parent.Player.Text
		
		local args = {
			[1] = game:GetService("Players")[tostring(PlayerNamex)]
		}
	
		game:GetService("ReplicatedStorage").GameEvents.PartyJoin:FireServer(unpack(args))
	end)
end
coroutine.wrap(KKFXSHJ_fake_script)()
local function JSDGPX_fake_script() -- Invite.Invite 
	local script = Instance.new('LocalScript', Invite)

	script.Parent.MouseButton1Down:Connect(function()
		local PlayerNamex = script.Parent.Parent.Player.Text
		local args = {
			[1] = game:GetService("Players")[tostring(PlayerNamex)]
		}
	
		game:GetService("ReplicatedStorage").GameEvents.PartyInvite:FireServer(unpack(args))
	end)
end
coroutine.wrap(JSDGPX_fake_script)()
local function TARRHT_fake_script() -- Players.Players 
	local script = Instance.new('LocalScript', Players)

	local function Template (Player)
		local Button = script.Parent.Parent.Parent.Storage.Players:Clone()
		local StatsPath = script.Parent.Parent.Stats
		local PlayerName = Player.Name
		local PlayerId = Player.UserId
		Button.Parent = script.Parent.Parent.Players.PlayList
		Button.Name = Player.Name
		Button.Text = Player.Name
		Button.Visible = true
		Button.MouseButton1Down:Connect(function()
			StatsPath.Level.Text = "Name : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Stats.Rank.Value)
			StatsPath.PlayersName.Text = "Level : "..tostring(PlayerName)
			StatsPath.Kills.Text = "Kills : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Stats.Kills.Value)
			StatsPath.Deaths.Text = "Deaths : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Stats.Deaths.Value)
			StatsPath.Cash.Text = "Cash : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Stats.Cash.Value)
			StatsPath.Tokens.Text = "Tokens : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Stats.Tokens.Value)
			StatsPath.Reports.Text = "Reports : "..tostring(game.Players[tostring(PlayerName)].PlayerData.Report.Hacking.Value)
			StatsPath.Player.Text = PlayerName
			StatsPath.PlayerPicture.Image = "rbxthumb://type=AvatarHeadShot&id="..PlayerId.."&w=100&h=100"
		end)
	end
	
	game.Players.PlayerAdded:Connect(function(plr)
		Template(plr)
	end)
	
	for _,Players in pairs(game.Players:GetChildren()) do
		Template(Players)
	end
	
	game.Players.PlayerRemoving:Connect(function(plr)
		for i,v in pairs(script.Parent.Parent.Players.PlayList:GetChildren()) do
			if v.Name and plr.Name then
				v.Name:Destroy()
			end
		end
	end)
	
end
coroutine.wrap(TARRHT_fake_script)()
